{
    "name": "NikolaiJAlexander.com",
    "version": "1.0.0",
    "description": "This is a website for hosting my resume.",
    "scripts": {
      "build-base-css": "node-sass core_app/static/scss/base.scss core_app/static/css/base.css",
      "build-index-css": "node-sass core_app/static/scss/index.scss core_app/static/css/index.css",
      "build-navbar-css": "node-sass core_app/static/scss/navbar.scss core_app/static/css/navbar.css",
      "build-core-css": "npm run build-base-css && npm run build-index-css && npm run build-navbar-css",
      "build-css": "npm run build-core-css"
    },
    "devDependencies": {
      "node-sass": "^9.0.0"
    }
  }
